{
  "name": "rim-harmony",
  "version": "1.0.0",
  "description": "An app to check rimworld mods harmony version",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [
    "rimworld"
  ],
  "author": {
    "name": "Alexandre Frizac"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "build/webpack.main.config.js",
            "renderer": {
              "config": "build/webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./source/view/index.html",
                  "js": "./source/view/renderer.ts",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.1",
    "@types/he": "^1.1.1",
    "bootstrap": "^4.5.0",
    "bootstrap-vue": "^2.15.0",
    "compare-versions": "^3.6.0",
    "electron-store": "^5.2.0",
    "exiftool2": "^1.0.1",
    "expose-loader": "^0.7.5",
    "he": "^1.2.0",
    "jquery": "^3.5.1",
    "klaw": "^3.0.0",
    "lodash": "^4.17.15",
    "popper.js": "^1.16.1",
    "stylus-loader": "^3.0.2",
    "uuid": "^8.2.0",
    "vue": "^2.6.11",
    "vue-i18n": "^8.18.2",
    "vue-router": "^3.3.4",
    "vuex": "^3.4.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.51",
    "@electron-forge/maker-zip": "^6.0.0-beta.51",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.51",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/chai": "^4.2.11",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/klaw": "^3.0.1",
    "@types/lodash": "^4.14.157",
    "@types/mocha": "^7.0.2",
    "@types/node": "^12.12.47",
    "@types/uuid": "^8.0.0",
    "@types/webpack": "^4.41.18",
    "@types/webpack-env": "^1.15.2",
    "@types/xml2js": "^0.4.5",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "chai": "^4.2.0",
    "css-loader": "^3.6.0",
    "electron": "^9.3.1",
    "electron-devtools-installer": "^3.0.0",
    "eslint": "^7.3.1",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^4.1.6",
    "less-loader": "^6.1.2",
    "mocha": "^7.2.0",
    "node-loader": "^0.6.0",
    "style-loader": "^1.2.1",
    "stylus": "^0.54.7",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.49",
    "typescript": "^3.9.5",
    "url-loader": "^4.1.0",
    "vue-class-component": "^7.2.3",
    "vue-loader": "^15.9.3",
    "vue-property-decorator": "^8.5.1",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-permissions-plugin": "^1.0.7"
  }
}
